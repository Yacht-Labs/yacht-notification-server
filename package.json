{
  "name": "yacht-notification-service",
  "version": "1.0.0",
  "description": "yacht notification service",
  "main": "./out/src/server/index.js",
  "engines": {
    "node": "^16.0.0"
  },
  "scripts": {
    "test": "jest -i",
    "build": "tsc",
    "db-fix": "npm run db-reset && npm run db-deploy",
    "start": "tsc && node ./out/src/server/index.js",
    "db-init": "ts-node src/scripts/initTokenDb.ts",
    "db-migrate": "ts-node prisma/scripts/migrateScript.ts",
    "db-generate": "ts-node prisma/scripts/generateScript.ts",
    "db-deploy": "ts-node prisma/scripts/deployScript.ts",
    "db-view": "ts-node prisma/scripts/studioScript.ts",
    "db-reset": "ts-node prisma/scripts/resetScript.ts",
    "db-seed": "ts-node prisma/scripts/seedScript.ts",
    "dev": "nodemon src/server/index.ts",
    "start-production": "npm run db-deploy && npm start"
  },
  "author": "Yacht Labs Inc.",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/morgan": "^1.9.4",
    "@types/morgan-json": "^1.1.0",
    "@types/node-cron": "^3.0.2",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.3",
    "jest": "^28.1.3",
    "jest-mock-extended": "^2.0.7",
    "nodemon": "^2.0.19",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "apn": "^2.2.0",
    "bignumber.js": "^9.1.0",
    "dotenv": "^16.0.1",
    "ethers": "^5.7.0",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "ipfs-core": "^0.14.0",
    "lit-swap-sdk": "1.1.13",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "node-cron": "^3.0.2",
    "node-fetch": "^2.6.7",
    "prisma": "^4.3.1",
    "winston": "^3.8.2",
    "winston-cloudwatch": "^6.1.1"
  }
}
